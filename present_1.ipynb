gme_revenue["Date"] = pd.to_datetime(gme_revenue["Date"])
gme_data["Date"] = pd.to_datetime(gme_data["Date"])

fig = go.Figure()

fig.add_trace(go.Scatter(
    x=gme_data["Date"],
    y=gme_data["Close"],
    name="GME Close Price",
    mode="lines"
))

fig.add_trace(go.Scatter(
    x=gme_revenue["Date"],
    y=gme_revenue["Revenue"],
    name="GameStop Revenue",
    mode="lines+markers",
    yaxis="y2"
))

fig.update_layout(
    title="GameStop: Stock Price vs Revenue",
    xaxis_title="Date",
    yaxis=dict(title="Stock Price (USD)"),
    yaxis2=dict(title="Revenue (USD)", overlaying="y", side="right")
)

fig.show()
